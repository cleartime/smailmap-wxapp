.main{
  height: 100vmax;
  width: 100%;
}
.main .head{
  z-index: 100;
  position: fixed;
  top: 10%;
  left: 5%;
  right: 0;
  display: flex;
  justify-content: space-around;
  padding: 15px 0;
  width: 90%;
  align-items: center;
  border-radius: 10px
}
.main .head .img1{
  width: 33px;
  height: 33px
}
.main .head .img2{
  width: 20px;
  height: 20px;
}
.main .head .text{
  font-size: 16px;
  color: #666666;
}
.main .head .img1, .main .head .img2, .main .head .text{
  z-index: 100;
}
.main .head .bg{
  position: absolute;
  z-index: 1;
}
.left{
  position: fixed;
  left: 15px;
  top: 50%;
  z-index: 100;
}
.left .img{
  width: 35px;
  height: 35px;
}
.right{
  position: fixed;
  right: 15px;
  top: 50%;
  z-index: 100;
}
.right .top, .right .bottom{
  width: 36.5px;
  height: 36.5px;
}
map{
  height: 100%;
  width: 100%;
}